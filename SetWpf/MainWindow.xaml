            <ComboBoxItem Name="union">UNION</ComboBoxItem>
            <ComboBoxItem Name="intersection" >INTERSECTION</ComboBoxItem>
            <ComboBoxItem Name="difference">DIFFERENCE</ComboBoxItem>
            <ComboBoxItem Name="symetricDif">SYMETRIC DIF</ComboBoxItem>
        </ComboBox>
        <ComboBox HorizontalAlignment="Left" Margin="431,45,0,0" VerticalAlignment="Top" Width="120"/>
        <Button Content="Evaluate" HorizontalAlignment="Left" Margin="609,52,0,0" VerticalAlignment="Top" Width="120" Click="Evaluate"/>
        <ListBox  Margin="0,103,591,0"/>
        <ListBox  Margin="307,115,307,0"/>
        <ListBox  Margin="588,131,0,0"/>

    </Grid>
</Window>
